{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false
  },
  "projects": {
    "frontend": {
      "type": "application",
      "root": "src/frontend",
      "entryFile": "main",
      "sourceRoot": "src/frontend",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    },
    "dashboard": {
      "type": "application",
      "root": "src/dashboard",
      "entryFile": "main",
      "sourceRoot": "src/dashboard",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    },
    "excel": {
      "type": "application",
      "root": "src/excel",
      "entryFile": "main",
      "sourceRoot": "src/excel",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    },
    "sms-sending": {
      "type": "application",
      "root": "src/sms-sending",
      "entryFile": "main",
      "sourceRoot": "src/sms-sending",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    }
  }
}
